<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<converter>
		<converter-id>conversorEstado</converter-id>
		<converter-class>tcc.conversores.ConversorEstado</converter-class>
	</converter>
	<converter>
		<converter-id>conversorCidade</converter-id>
		<converter-class>tcc.conversores.ConversorCidade</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>cidadesBEAN</managed-bean-name>
		<managed-bean-class>tcc.beans.CidadesBEAN</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>empresasBEAN</managed-bean-name>
		<managed-bean-class>tcc.beans.EmpresasBEAN</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>cadastros/gerenciarEmpresas.xhtml</display-name>
		<from-view-id>/cadastros/gerenciarEmpresas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/cadastros/gerenciarEmpresas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cadastros/gerenciarEmpresas.xhtml</display-name>
		<from-view-id>/cadastros/gerenciarEmpresas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>paginaDetalhesEmpresa</from-outcome>
			<to-view-id>/cadastros/detalhesEmpresa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
